{
  "version": 2,
  "builds": [
    {
      "src": "api/sendData.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/getData.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sendData",
      "dest": "/api/sendData.js"
    },
    {
      "src": "/api/getData",
      "dest": "/api/getData.js"
    }
  ]
}
